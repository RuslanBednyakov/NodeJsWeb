<% layout('/layout/page') -%>
<% block('title').append('Search') %>
<% if (user) { %>
  <% block('script').append('<script src="api/v1/myPage/user.js"></script>') %>
<% } %>

<div class="search__container">
  <div class="search__container_tittle">
    <h3 class="search__container_tittle-header">Search result: <span id="search__count"></span> users found </h3>
  </div>
  <div class="search__container_results">
    <ul class="search__container_results-list" id="search__list"></ul>
  </div>
</div>